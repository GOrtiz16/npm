{
  "name": "shell",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local",
    "start:dev": "ng serve -c dev",
    "start:cert": "ng serve -c cert",
    "start:prod": "ng serve -c production",
    "build": "ng build -c local",
    "build:prod": "ng build -c production",
    "build:production": "ng build -c production",
    "build:cert": "ng build -c cert",
    "build:staging": "ng build -c cert",
    "build:dev": "ng build -c dev",
    "build:development": "ng build -c dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "test:deploy": "ng test --watch=false --coverage",
    "prepare": "husky install",
    "lint": "npx lint \"src/**/*.{js,jsx,ts,tsx,html}\" --quiet --fix",
    "lint:fix": "npx lint \"src/**/*.{js,jsx,ts,tsx,html}\" --fix ",
    "prettier": "npx pretty-quick --staged && npx lint \"src/**/*.{js,jsx,ts,tsx,html}\" --fix"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "~14.3.14",
    "@angular-architects/module-federation-tools": "~14.3.10",
    "@angular/animations": "~14.2.11",
    "@angular/cdk": "^13.0.0",
    "@angular/common": "~14.2.11",
    "@angular/compiler": "~14.2.11",
    "@angular/core": "~14.2.11",
    "@angular/forms": "~14.2.11",
    "@angular/material": "^13.0.0",
    "@angular/platform-browser": "~14.2.11",
    "@angular/platform-browser-dynamic": "~14.2.11",
    "@angular/router": "~14.2.11",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "ngx-mask": "^14.2.4",
    "rxjs": "~7.5.0",
    "tslib": "~2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "14.0.0",
    "@angular-devkit/build-angular": "~14.2.11",
    "@angular-eslint/eslint-plugin-template": "14.2.0",
    "@angular/cli": "~14.2.11",
    "@angular/compiler-cli": "~14.2.11",
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "28.1.1",
    "ngx-build-plus": "~14.0.0",
    "prettier": "^2.8.6",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "typescript": "4.6.2"
  }
}
